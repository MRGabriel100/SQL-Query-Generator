{
  "create": 
     [
        {
          "name": "CREATE DATABASE",
          "template": "CREATE DATABASE {database_name}"
        },
        {
          "name": "CREATE TABLE",
          "template": "CREATE TABLE {table_name} ({columns})"
        },
        {
          "name": "CREATE TEMPORARY TABLE",
          "template": "CREATE TEMPORARY TABLE {table_name} ({columns})"
        },
        {
          "name": "CREATE INDEX",
          "template": "CREATE INDEX {index_name} ON {table_name} ({columns})"
        },
        {
          "name": "CREATE UNIQUE INDEX",
          "template": "CREATE UNIQUE INDEX {index_name} ON {table_name} ({columns})"
        },
        {
          "name": "CREATE FULLTEXT INDEX",
          "template": "CREATE FULLTEXT INDEX {index_name} ON {table_name} ({columns})"
        },
        {
          "name": "CREATE SPATIAL INDEX",
          "template": "CREATE SPATIAL INDEX {index_name} ON {table_name} ({columns})"
        },
        {
          "name": "CREATE VIEW",
          "template": "CREATE VIEW {view_name} AS {select_query}"
        },
        {
          "name": "CREATE OR REPLACE VIEW",
          "template": "CREATE OR REPLACE VIEW {view_name} AS {select_query}"
        },
        {
          "name": "CREATE TRIGGER",
          "template": "CREATE TRIGGER {trigger_name} {timing} {event} ON {table_name} FOR EACH ROW {trigger_body}"
        },
        {
          "name": "CREATE PROCEDURE",
          "template": "CREATE PROCEDURE {procedure_name} ({parameters}) BEGIN {body} END"
        },
        {
          "name": "CREATE FUNCTION",
          "template": "CREATE FUNCTION {function_name} ({parameters}) RETURNS {return_type} BEGIN {body} END"
        },
        {
          "name": "CREATE EVENT",
          "template": "CREATE EVENT {event_name} ON SCHEDULE {schedule} DO {event_body}"
        },
        {
          "name": "CREATE USER",
          "template": "CREATE USER '{username}'@'{host}' IDENTIFIED BY '{password}'"
        }
      ]
    
  ,

